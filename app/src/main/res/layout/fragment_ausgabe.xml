<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AusgabeFragment"
    android:background="@color/colorPrimary"
    android:clickable="true">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/aussentemperatur"
            android:textSize="20dp"
            android:id="@+id/ausgabe_eingegebene_temp"
            android:layout_marginTop="30dp"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/relative_luftfeuchtigkeit"
            android:textSize="20dp"
            android:id="@+id/ausgabe_eingegebene_humidity"
            />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/ergebnis"
            android:textSize="20dp"
            android:id="@+id/ausgabe_ergebnis"
            />
    </LinearLayout>

</FrameLayout>